local:awardInOrganizationContext a :ConfigContext ;
    :hasConfiguration local:awardInOrganizationConfig ;
    :configContextFor vlocal:has_honored_member ;
    :qualifiedByDomain foaf:Organization ;
    :qualifiedBy foaf:Person .

